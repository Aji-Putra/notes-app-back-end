{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-console": "off",
        "eqeqeq": "off",
        "curly": "error",
        "quotes": ["error", "double"],
        "linebreak-style": 0
    }
}
